{
  "name": "csc",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host=0.0.0.0 --port=8080",
    "start-nhc": "ng serve --host=0.0.0.0 --port=8080 --disable-host-check",
    "deploy": "node ./scripts/deploy.js",
    "gen:img": "node ./src/createImageConfigs.js",
    "build": "ng build --prod --aot && npm run pwa",
    "build:dev": "ng build --aot && npm run pwa",
    "deploy:dev": "firebase deploy",
    "dev": "npm run build:dev && npm run deploy:dev",
    "prod": "npm run build && npm run postbuild && npm run deploy",
    "prod:dev": "npm run build:dev && npm run postbuild && npm run deploy:dev",
    "postbuild": "bash ./scripts/postbuild.sh",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "pwa": "./node_modules/.bin/sw-precache --root=dist --config=precache-config.js",
    "format:fix": "pretty-quick && ng lint --fix",
    "format:check": "pretty-quick --check && ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^12.1.1",
    "@angular/cdk": "^12.1.1",
    "@angular/common": "^12.1.1",
    "@angular/compiler": "^12.1.1",
    "@angular/core": "^12.1.1",
    "@angular/fire": "^6.1.5",
    "@angular/forms": "^12.1.1",
    "@angular/material": "^12.1.1",
    "@angular/platform-browser": "^12.1.1",
    "@angular/platform-browser-dynamic": "^12.1.1",
    "@angular/router": "^12.1.1",
    "core-js": "^2.6.12",
    "firebase": "^8.7.1",
    "font-awesome": "^4.7.0",
    "hammerjs": "^2.0.8",
    "intersection-observer": "^0.4.2",
    "magic-check": "^1.0.3",
    "mkdirp": "^1.0.4",
    "ngx-gallery": "^5.10.0",
    "ngx-gallery-9": "^1.0.6",
    "ngx-markdown": "^12.0.1",
    "normalize.css": "^7.0.0",
    "popper.js": "^1.12.5",
    "rxjs": "^6.6.7",
    "sass": "^1.35.2",
    "tslib": "^1.14.1",
    "webpack": "^5.44.0",
    "zone.js": "^0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^12.1.1",
    "@angular/cli": "^12.1.1",
    "@angular/compiler-cli": "^12.1.1",
    "@angular/language-service": "^12.1.1",
    "@types/jasmine": "^2.8.18",
    "@types/jasminewd2": "^2.0.10",
    "@types/node": "^12.20.16",
    "base64-img": "^1.0.3",
    "codelyzer": "^6.0.2",
    "gh-pages": "^1.0.0",
    "husky": "^4.3.8",
    "jasmine-core": "^2.8.0",
    "jasmine-spec-reporter": "^4.2.1",
    "jimp": "^0.16.1",
    "karma": "^6.3.4",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "^4.0.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "prettier": "^2.3.2",
    "pretty-quick": "^2.0.2",
    "protractor": "^7.0.0",
    "svg2png": "^4.1.1",
    "sw-precache": "^5.2.0",
    "sw-precache-webpack-plugin": "^0.11.4",
    "ts-node": "^3.3.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.3.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "ng lint --fix && pretty-quick --staged"
    }
  }
}
